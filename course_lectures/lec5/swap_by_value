#include <iostream>
using namespace std;


int main ()
{
    int x=3;
    int y=4;
    void swap(int ,int);
    cout<<"old val (x,y)"<<"("<<x<<","<<y<<")"<<endl;
    swap(x,y);
    cout<<"the val after calling swap (x,y)"<<"("<<x<<","<<y<<")"<<endl; //it will not change

    return 0;
}
void swap(int x,int y)
{

    int temp;
    temp=x;
    x=y;
    y=temp;
}
