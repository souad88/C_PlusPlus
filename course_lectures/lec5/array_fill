//cppreference.com/w/cpp/container/array
//fill_method
#include <iostream>
#include<array>
using namespace std;
int main()
{
        
        array<string,3> names={"ahmed","mohammed","abd-elhady"};
        names.fill("Fayrouz");
        for(string i :names)
        {
                cout<<i<<endl;
        }
        return 0;
}
