//swap_method
#include<iostream>
#include<array>
using namespace std;

int main()
{
        array<string,3> days={"first day","seconed day","third day"};
        array<string,3> days_={"saturday","sunday","monday"};
        days.swap(days_);
        for(int i=0;i<3;i++)
        {
            cout<<"days "<<days[i]<<endl;
            cout<<"days_"<<days_[i]<<endl;
        }
        return 0;
}
