#include<iostream>
using namespace std;
int main()
{
        
        //lambda to add two numbers
        auto lambda=[](int a,int b,int c)->int {
                                            c=a+b;
                                            return c;

                                         };

        int res=lambda(4,3,0);
        cout<<"result ="<<res<<endl;                                 
        return 0;
}
