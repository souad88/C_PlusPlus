using namespace std;
int main()
{
        float x1,x2,y1,y2;
        cout<<"enter x of point_1"<<endl;
        cin>>x1;
        cout<<"enter y of point_1"<<endl;
        cin>>y1;
        cout<<"enter x of point_2"<<endl;
        cin>>x2;
        cout<<"enter y of point_2"<<endl;
        cin>>y2;
        float m;
        m=(y2-y1)/(x2-x1);
        cout<<"m o this line  "<<m<<endl;
        return 0;
}
